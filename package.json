{
  "name": "dndwm",
  "version": "0.0.1",
  "description": "Dungeons & Dragons World Manager",
  "main": "client/src/index.js",
  "scripts": {
    "clean": "rimraf client/dist",
    "build-js": "node node_modules/webpack/bin/webpack --optimize-minimize",
    "build-css": "lessc --clean-css client/src/index.less client/dist/style.min.css",
    "build": "npm run clean & npm run build-css & npm run build-js",
    "test": "jest"
  },
  "jest": {
    "scriptPreprocessor": "preprocessor.js",
    "unmockedModulePathPatterns": [
      "react"
    ]
  },
  "repository": {
    "type": "git",
    "url": "(https://github.com/roerdinkholder/dndwm.git)"
  },
  "author": "Hans Roerdinkholder",
  "license": "ISC",
  "dependencies": {
    "babel": "^5.8.23",
    "babel-core": "^5.8.25",
    "babel-loader": "^5.3.2",
    "react": "^0.14.0",
    "react-dom": "^0.14.0",
    "react-redux": "^3.1.0",
    "react-router": "^1.0.0-rc3",
    "redux": "^3.0.2",
    "redux-thunk": "^1.0.0",
    "webpack": "^1.12.2"
  },
  "devDependencies": {
    "compression-webpack-plugin": "^0.2.0",
    "jest-cli": "^0.5.10",
    "less": "^2.5.3",
    "less-plugin-clean-css": "^1.5.1",
    "redux-devtools": "^2.1.5",
    "rimraf": "^2.4.3"
  }
}
